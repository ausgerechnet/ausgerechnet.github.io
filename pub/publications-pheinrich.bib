 title = {{EmpiriST} {Corpus} 2.0: {Adding} {Manual} {Normalization,} {Lemmatization} and {Semantic} {Tagging} to a {German} {Web} and {CMC} {Corpus}},
 url = {https://www.aclweb.org/anthology/2020.lrec-1.754},
